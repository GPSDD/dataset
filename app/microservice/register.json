{
	"name": "Dataset microservice",
	"tags": ["dataset"],
	"cache": ["dataset"],
	"uncache": ["dataset"],
	"endpoints": [{
		"path": "/v1/dataset",
		"method": "GET",
		"authenticated": true,
		"redirect": {
			"method": "GET",
			"path": "/api/v1/dataset"
		}
	},{
		"path": "/v1/dataset/find-by-ids",
		"method": "POST",
		"authenticated": true,
		"redirect": {
			"method": "POST",
			"path": "/api/v1/dataset/find-by-ids"
		}
	},{
		"path": "/v1/dataset",
		"method": "POST",
		"authenticated": true,
		"redirect": {
			"method": "POST",
			"path": "/api/v1/dataset"
		}
	},{
		"path": "/v1/dataset/:dataset",
		"method": "GET",
		"authenticated": true,
		"redirect": {
			"method": "GET",
			"path": "/api/v1/dataset/:dataset"
		}
	},{
		"path": "/v1/dataset/:dataset",
		"method": "PATCH",
		"authenticated": true,
		"redirect": {
			"method": "PATCH",
			"path": "/api/v1/dataset/:dataset"
		}
	},{
		"path": "/v1/dataset/:dataset",
		"method": "DELETE",
		"authenticated": true,
		"redirect": {
			"method": "DELETE",
			"path": "/api/v1/dataset/:dataset"
		}
	},{
		"path": "/v1/dataset/:dataset/clone",
		"method": "POST",
		"authenticated": true,
		"redirect": {
			"method": "POST",
			"path": "/api/v1/dataset/:dataset/clone"
		}
	},{
		"path": "/v1/dataset/upload",
		"method": "POST",
		"authenticated": true,
		"redirect": {
			"method": "POST",
			"path": "/api/v1/dataset/upload"
		}
	}, {
		"path": "/v1/dataset/:dataset/verification",
		"method": "GET",
		"authenticated": false,
		"redirect": {
			"method": "GET",
			"path": "/api/v1/dataset/:dataset/verification"
		}
	}]
}
